                "dsDescriptionValue": {
                  "typeName": "dsDescriptionValue",
                  "multiple": false,
                  "typeClass": "primitive",
                  "value": "The University Student Mental Health data was gathered during the fall of 2020. This online survey included sections on demographics, mental health, changes in mental health during the pandemic, self-care habits (sleep, exercise, emotion regulation, social support, and mindfulness), and both recreation (athletics partying, games, and watching online videos) and academic hobbies (studying, participating in academic organizations, attending office hours, and using educational resources)."
                },
                "dsDescriptionDate": {
                  "typeName": "dsDescriptionDate",
                  "multiple": false,
                  "typeClass": "primitive",
                  "value": "2021-10-10"
                }
              }
